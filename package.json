    "name": "web-application-template",
    "overrides": {
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/webextensions/web-application-template.git"
    },
    "scripts": {
        "all-is-well": "./scripts/health-checks/all-is-well.sh",
        "build": "npm run build:development:local",
        "build:development:local": "webpack     --mode development --watch --progress --config ./frontend/webpack.config.js --env config=\"./config/config.development.local.js\"",
        "build:development:local:inspect-brk": "node --inspect-brk ./node_modules/.bin/webpack-cli --mode development         --progress --config ./frontend/webpack.config.js --env config=\"./config/config.development.local.js\"",
        "build:inspect-brk": "npm run build:development:local:inspect-brk",
        "build:production:local": "NODE_ENV=production                                        webpack     --mode production  --watch --progress --config ./frontend/webpack.config.js --env config=\"./config/config.production.local.js\"",
        "build:production:local:inspect-brk": "NODE_ENV=production node --inspect-brk ./node_modules/.bin/webpack-cli --mode production          --progress --config ./frontend/webpack.config.js --env config=\"./config/config.production.local.js\"",
        "housekeeping:clean": "./scripts/housekeeping/clean.js",
        "housekeeping:review-dependencies": "./scripts/housekeeping/imperfect/review-dependencies-via-depcheck.sh",
        "housekeeping:update-package-cjson-and-json": "./scripts/housekeeping/update-package-cjson-and-json.sh",
        "housekeeping:update-package-lock-json": "./scripts/housekeeping/update-package-lock-json.sh",
        "lint": "eslint --flag unstable_config_lookup_from_file .",
        "lint:fix": "eslint --flag unstable_config_lookup_from_file . --fix",
        "mocha": "mocha",
        "postversion": "git push --follow-tags",
        "preinstall": "./scripts/npm-run-scripts/preinstall.sh",
        "prepare": "husky || true",
        "server:development:local": "nodemon            backend/src/server/server.js -- --config config/config.development.local.js",
        "server:development:local:inspect-brk": "node --inspect-brk backend/src/server/server.js    --config config/config.development.local.js",
        "server:development:local:use-hmr": "USE_HMR=yes nodemon            backend/src/server/server.js -- --config config/config.development.local.js",
        "server:production:local": "NODE_ENV=production nodemon            backend/src/server/server.js -- --config config/config.production.local.js",
        "server:production:local:inspect-brk": "NODE_ENV=production node --inspect-brk backend/src/server/server.js    --config config/config.production.local.js",
        "start": "npm run start:app",
        "start:app": "concurrently \"npm run start:server\" \"npm run start:build\"",
        "start:app:use-hmr": "npm run start:server:use-hmr",
        "start:build": "npm run build",
        "start:build:inspect-brk": "npm run build:inspect-brk",
        "start:proxy:localtunnel:8000": "printf \"Your IP is:  \" && curl https://loca.lt/mytunnelpassword && echo \" (Tunnel Password)\" ; npx --yes --prefer-offline localtunnel --port 8000",
        "start:proxy:serveo:8000": "ssh -R 80:localhost:8000 serveo.net",
        "start:server": "npm run server:development:local",
        "start:server:inspect-brk": "npm run server:development:local:inspect-brk",
        "start:server:use-hmr": "npm run server:development:local:use-hmr",
        "test": "npm run all-is-well",
        "version": "./scripts/build-and-release/prepare-version/prepare-version.sh"
    },
    "type": "module",
    "version": "1.0.0"
}
